
@{
    ViewBag.Title = "UploadProfilePicture";
}

<h2>Upload Profile Picture</h2>

<form method="post" enctype="multipart/form-data">
    <label class="control-label">Upload file</label>
    <div class="form-group">
        <input type="file" name="picture" class="form-control"/>
    </div>
    <div class="form-group">
        <input type="submit" value="Save" class="btn btn-default" formaction="UploadProfilePicture"/>
    </div>
</form>

<button class="btn btn-danger" onclick="location.href='@Url.Action("Index", "Feed")'">Cancel</button>

